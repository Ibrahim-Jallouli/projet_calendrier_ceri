<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.m1prototypage.controller.AddSeanceForm"
            prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="10" alignment="CENTER">
        <!-- Assurez-vous que le VBox est bien positionné dans l'AnchorPane -->
        <children>
            <TextField fx:id="nomSeance" promptText="Nom de la séance" />
            <Button text="Ajouter" onAction="#ajouterSeance" />
        </children>
    </VBox>
</AnchorPane>
